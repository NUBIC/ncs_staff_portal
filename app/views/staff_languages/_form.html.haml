= form_for @staff_language do |f|
  -if @staff_language.errors.any?
    #error_explanation
      %h2= "#{pluralize(@staff_language.errors.count, "error")} prohibited this staff_language from being saved:"
      %ul
        - @staff_language.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :staff_id
    = f.text_field :staff_id
  .field
    = f.label :lang_code
    = f.text_field :lang_code
  .actions
    = f.submit 'Save'
