!!! 1.1
%html
  %head>
    %title NCS Staff Portal
    %link{ :rel=>"icon", :type=>"image/gif", :href=>"/images/favicon.gif"}
    = stylesheet_link_tag :all
    = stylesheet_link_tag 'compiled/screen.css', :media => 'screen, projection'
    = stylesheet_link_tag 'compiled/print.css', :media => 'print'
    = javascript_include_tag('jquery-1.6.2.min', 'jquery-ui-1.8.7', 'application','YUI/2.9/yahoo-dom-event','YUI/2.9/element-min','YUI/2.9/datasource-min','YUI/2.9/datatable-min')
    = yield :javascript
    = yield :head
    = csrf_meta_tag
  %body.bp.ncs
    #container
      #header
        #app_name
          NCS Staff Portal
      #content
        = yield
      #footer
        = render("partials/footer")
    = render('partials/include_google_analytics') if Rails.env.production?